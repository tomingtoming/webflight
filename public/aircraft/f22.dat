REM F-22 RAPTOR
IDENTIFY "F-22_RAPTOR"
CATEGORY FIGHTER
AFTBURNR TRUE                 #HAVE AFTERBURNER?
THRAFTBN 36.0t                #THRUST WITH AFTERBURNER
THRMILIT 22.0t                #THRUST AT MILITARY POWER
WEIGHCLN 14.0t                #WEIGHT CLEAN
WEIGFUEL  8.0t                #WEIGHT OF FUEL
WEIGLOAD 15.0t                #WEIGHT OF PAYLOAD
FUELABRN 8.0kg                #FUEL CONSUMPTION WHEN USING AFTERBURNER
FUELMILI 0.45kg               #FUEL CONSUMPTION AT MILITARY POWER

COCKPITP  0.0m  1.4m   4.50m  #COCKPIT POSITION
LEFTGEAR -1.6m -1.62m -3.04m  #LEFT LANDING GEAR POSITION
RIGHGEAR  1.6m -1.62m -3.04m  #RIGHT LANDING GEAR POSITION
WHELGEAR  0.0m -1.66m  3.00m  #WHEEL POSITION
ARRESTER  0.0m -1.62m -7.28m
MACHNGUN  1.1m  0.65m  2.05m  #MACHINE GUN POSITION
GUNINTVL 0.04
SMOKEGEN -0.3m -0.63m -8.90m  #SMOKE GENERATOR POSITION
VAPORPO0  6.8m  0.15m -5.18m  #VAPOR POSITION (Wings Swept Back or no-VGW)
VAPORPO1  6.8m  0.15m -5.18m  #VAPOR POSITION (Wings Spread)
HTRADIUS  11.0m               #OUTSIDE SPHERE RADIUS


STRENGTH 10

CRITAOAP  20deg               #CRITICAL AOA POSITIVE
CRITAOAM -15deg               #CRITICAL AOA NEGATIVE

MAXCDAOA 40deg                #AOA at which Cd becomes the largest
FLATCLR1 3deg                 #Cl stays the same this angle beyond critical AOA (plus side)
FLATCLR2 3deg                 #Cl stays the same this angle beyond critical AOA (minus side)
CLDECAY1 15deg                #After flat Cl range, Cl decays to zero in this range of angle (plus side)
CLDECAY2 10deg                #After flat Cl range, Cl decays to zero in this range of angle (minus side)

CRITSPED 0.9MACH              #CRITICAL SPEED
MAXSPEED 2.5MACH              #MAXIMUM SPEED

HASSPOIL TRUE                 #HAS SPOILER?
RETRGEAR TRUE                 #HAS RETRACTABLE LANDING GEAR?
VARGEOMW FALSE                #HAS VARIABLE GEOMETRY WING?

CLVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cl
CDVARGEO 0.0                  #EFFECT OF VARIABLE GEOMETRY WING FOR Cd
CLBYFLAP 0.1                  #EFFECT OF FLAP FOR Cl
CDBYFLAP 0.2                  #EFFECT OF FLAP FOR Cd
CDBYGEAR 0.5                  #EFFECT OF GEAR FOR Cd
CDSPOILR 2.0                  #EFFECT OF SPOILER FOR Cd

WINGAREA 70m^2                #WING AREA

MXIPTAOA 23.0deg              #MAX INPUT AOA
MXIPTSSA 5.0deg               #MAX INPUT SSA
MXIPTROL 360.0deg             #MAX INPUT ROLL

REM CPITMANE corresponds to Spring Constant K
REM CPITSTAB corresponds to Damper Constant B
REM To be critically damped, B=2*sqrt(K)
REM 2% Settling Time=??

MANESPD1 80kt                 #MINIMUM MANEUVABLE SPEED
MANESPD2 120kt                #FULLY MANEUVABLE SPEED
PSTMSPD1 100kt                #MAXIMUM SPEED POST-STALL ATTITUDE CONTROL IS FULLY EFFECTIVE
PSTMSPD2 180kt                #SPEED AT WHICH POST-STALL ATTITUDE CONTROL IS INEFFECTIVE
CPITMANE 8.0                  #PITCH MANEUVERBILITY CONSTANT
CPITSTAB 2.0                  #PITCH STABILITY CONSTANT
CYAWMANE 5.0                  #YAW MANEUVABILITY CONSTANT
CYAWSTAB 3.0                  #YAW STABILITY CONSTANT
CROLLMAN 3.0                  #ROLL MANEUVABILITY CONSTANT
PSTMPTCH 90deg                #MAXIMUM PITCH ROTATION (PER SEC) THAT A THRUST VECTORING CAN PRODUCE
PSTMYAW_ 90deg                #MAXIMUM YAWING ROTATION (PER SEC) THAT A THRUST VECTORING CAN PRODUCE
PSTMROLL 90deg                #MAXIMUM ROLLING ROTATION (PER SEC) THAT A THRUST VECTORING CAN PRODUCE
PSTMPWR1 0.2                  #MINIMUM REQUIRED POWER SETTING FOR DIRECT ATTITUDE CONTROL
PSTMPWR2 0.8                  #POWER SETTING AT WHICH DIRECT ATTITUDE CONTROL IS FULLY EFFECTIVE

CTLLDGEA TRUE                 #CONTROL LANDINGGEAR
CTLBRAKE FALSE                #CONTROL BRAKE
CTLSPOIL 0.0                  #CONTROL SPOILER
CTLABRNR FALSE                #CONTROL AFTERBURNER
CTLTHROT 0.0                  #CONTROL THRUST
CTLIFLAP 0.0                  #CONTROL FLAP
CTLINVGW 0.0                  #CONTROL VARIABLE GEOMETRY WING
CTLATVGW FALSE                #CONTROL AUTO VARIABLE GEOMETRY WING

POSITION 0m 3ft 0m           #POSITION
ATTITUDE 45deg 0deg 0deg      #ATTITUDE
INITFUEL 75%                  #INITIAL FUEL
INITLOAD 0.0t                 #INITIAL PAYLOAD
INITSPED 0.0MACH              #INITIAL SPEED

REM Slots must come before Inits.
HRDPOINT  0.61m -0.95m -3.00m $INTERNAL AIM9 AIM9X AIM120
HRDPOINT -0.61m -0.95m -3.00m $INTERNAL AIM9 AIM9X AIM120
HRDPOINT  0.61m -0.95m  0.08m $INTERNAL AIM9 AIM9X AIM120 AGM65 B250
HRDPOINT -0.61m -0.95m  0.08m $INTERNAL AIM9 AIM9X AIM120 AGM65 B250
HRDPOINT  0.61m -0.95m -3.00m $INTERNAL AIM9 AIM9X AIM120 AGM65 B250
HRDPOINT -0.61m -0.95m -3.00m $INTERNAL AIM9 AIM9X AIM120 AGM65 B250
HRDPOINT  3.47m  0.00m -3.14m AIM9X
HRDPOINT -3.47m  0.00m -3.14m AIM9X
HRDPOINT  4.35m  0.00m -3.14m AIM9X
HRDPOINT -4.35m  0.00m -3.14m AIM9X
HRDPOINT  3.91m -0.31m -3.14m FUEL&1000 AIM9X
HRDPOINT -3.91m -0.31m -3.14m FUEL&1000 AIM9X

WPNSHAPE FUEL STATIC aircraft/f22_fueltank.srf
WPNSHAPE FUEL FLYING aircraft/f22_fueltank.srf

LMTBYHDP TRUE                 #Limit Max Num Weapon by Hardpoint


INITIGUN 3000                 #INITIAL GUN
LOADWEPN AIM120 2
LOADWEPN AIM9X 4




RADARCRS 0.1                  #RADAR CROSS SECTION

REM ClZero,CdZero is fixed by Cruising condition
REFVCRUS 0.9MACH              #CRUISING SPEED
REFACRUS 20000ft              #CRUISING ALTITUDE
REFTCRUS 0.8                  #CRUISING THROTTLE SETTING

REM ClSlope,CdConst is fixed by Landing condition
REFVLAND 110kt                #LANDING SPEED
REFAOALD 10deg                #LANDING AOA
REFLNRWY 2000m                #RUNWAY REQUIRED TO STOP
REFTHRLD 0.3                  #LANDING THROTTLE SETTING

AUTOCALC
